@article{lichbach2010,
	Abstract = {During his long and distinguished career, Charles Tilly addressed the problem situation he inherited from his teacher Barrington Moore, the situation that emerged in the middle of his studies, and the problems that arose later in his life. Moore's core themes were revolutionary classes, revolutionary violence, and the outcomes of revolution. As the 1960s and the 1970s gave way to the 1980s and 1990s, Tilly faced a different world-historical situation. So-called national liberation moments---violent and radical, communist and anti-U.S.---did not always win. And when they did succeed, radical politics was not particularly appealing. Tilly's final challenge involved rational-choice theory's drive for hegemony in explaining all outcomes---political, economic, and social---of macrohis- torical change. For example, could a systematic alternative to the major approach to contention and conflict---a bargaining theory of war---be developed? To address these changing problem situations, Tilly fashioned his own unique theories, methods, and domains of inquiry. A truly seminal thinker, he pioneered now standard social-scientific approaches to mechanisms, contentious politics, and state construction. To understand Charles Tilly is therefore to understand the last fifty years of historical and comparative social science.},
	Author = {Lichbach, Mark},
	Date-Added = {2016-05-09 13:27:18 +0000},
	Date-Modified = {2016-12-30 21:40:18 +0000},
	Journal = {Perspectives on Politics},
	Number = {02},
	Pages = {543--549},
	Publisher = {Cambridge {U}niv {P}ress},
	Title = {Charles Tilly's Problem Situations: From Class and Revolution to Mechanisms and Contentious Politics},
	Volume = {8},
	Year = {2010},
	Bdsk-File-1 = {}}

@article{mcadam09,
	Abstract = {In this chapter, McAdam, Tarrow, and Tilly \citeyear{mcadam09} (MTT) argue for moving the field of ``contentious politics'' forward.  They trace the evolution of the field of comparative politics and highlight how the field of contentious politics differs.  Common properties to all contentious politics---and not to all comparative politics---are interactions, claims, and governments.  The authors explain the importance that interactions have on individuals and governments, and ultimately how such interactions affect the government.

This chapter is an excellent review of the process that the field of comparative politics has followed that leads to contentious politics.  Notably, MTT explain the political process approach, championed by Theda Skocpol's work on revolutions.  A rational choice and resource mobilization approach led by Mancur Olson opened the field to econometric explanations of contention.  Olson, although seeking an answer to collective action, mainly worked at the individual level.  Contention in comparative politics grew out of a paradigm shift away from structural models.  This shift led the way to ethnographic models focused in the Third World.  Starting in the 1980's, comparativsts and international relations scholars began to study the processes that drive political change.  Mobilization and demobilization have different clusters of mechanisms, and social movements.  MTT recommend a synthesis that includes two moves.  First, recognize that revolutions, social movements, mobilization, civil wars and democratization have common causal properties.  Second, recognize that different combinations of the causal properties create different social movement outcomes that range from violent to non-violent, routine to extraordinary, and from conservative to transformative.},
	Author = {McAdam, Doug and Tarrow, Sidney and Tilly, Charles},
	Date-Added = {2016-05-09 11:24:16 +0000},
	Date-Modified = {2016-05-09 12:53:07 +0000},
	Journal = {Comparative Politics Second Edition},
	Keywords = {Comparative Politics, Collective Action, Conflict},
	Pages = {260},
	Title = {Comparative perspectives on contentious politics},
	Year = {2009}}

@book{lichbach1995a,
	Abstract = {Chapter 1:
The Deprived Actor (DA) and Collective Action (CA) problems plague the academic literature.  The DA problem cannot suitably explain collective dissent.  Likewise, the CA theory fails to adequately explain why the collective dissent transforms into rebellion.  \emph{The Rebel's Dilemma} attempts to reconcile these shortcomings and, ``bring the richness of the CA research program to bear on conflict studies.'' \cite[13]{lichbach98}

Chapter 2:
In this chapter, \citeasnoun{lichbach98} offers a definition and solutions to the Rebel's Dilemma.  He delineates between short term solutions long term solutions.  Short term solutions are primarily marked by competition between tactics that rebels and government employ to overcome the collective action problem (in the case of the rebels) and prevent dissidents from overcoming the collective action problem (in the case of the government).  \citeasnoun{lichbach98} also offers an explanation to what situations after rebellion.  Codifying the purpose of the rebellion in internal politics is often very different from collective dissent, action, and victory.  According to \citeasnoun[29]{lichbach98}, ``CA solutions are self defeating because they lead to numerous pathologies of collective dissent.'' Lichbach continues, ``Several unintended and unwanted consequences follow from the dissident group's attempts to overcome the CA problem.''  Politics is a completely different game than rebellion.
},
	Author = {Lichbach, Mark Irving},
	Date-Added = {2016-05-09 10:58:34 +0000},
	Date-Modified = {2016-12-30 21:40:18 +0000},
	Keywords = {Insurgency, Collective Action, Comparative Politics, Conflict},
