@article{lichbach2008,
	Abstract = {Doug McAdam, Sidney Tarrow, and Charles Tilly's (MTT) Dynamics of Contention is a major achievement in conflict studies and indeed the social sciences. Their goal is to ``treat social interaction, social ties, communication, and conversation not merely as expressions of structure, rationality, consciousness, or culture but as active sites of creation and change. We have come to think of interpersonal networks, interpersonal communication, and various forms of continuous negotiation---including the negotiation of identities---as figuring centrally in the dynamics of contention'' (McAdam et al. 2001 p. 22). Contention is thus innovative: it turns conventional social action into contentious politics. Contention is dynamic: multiple streams of conflict and cooperation come together. Most importantly, contention is relational: a society of individuals, and neither just individuals nor just society, characterize contention.
MTT are less interested in determining which variables matter in a regression equation than in exploring how these constructive social processes operate. While they suggest that variables are most often a poor substitute and shallow stand-in for the causal mechanisms that account for the dynamics, relations, and innovations that give contention its most interesting and important features, their measurement models of mechanisms of contention could potentially turn constructivist insights into hypotheses, causal accounts, and research designs. MTT thus offer us positivist constructivism---a third way between positivist correlationalism and humanist constructivism.
Assuming that this is indeed their agenda, I aim to remind MTT and their readers of three things. First, measuring mechanisms is only a part of thinking and working with the entities, properties, events, structures, and situations associated with the causal grittiness of mechanisms. Second, successful causal accounts combine mechanism-based explanations with variable-based explanations, linking processes to hypotheses. Finally, quantitative and qualitative research designs probe the counterfactuals and the manipulations of the world involved in causal-mechanism accounts of contention.},
	Author = {Lichbach, Mark Irving},
	Date-Added = {2016-05-09 13:32:33 +0000},
	Date-Modified = {2016-12-30 21:40:33 +0000},
	Journal = {Qualitative Sociology},
	Number = {4},
	Pages = {345--354},
	Publisher = {Springer},
	Title = {Modeling mechanisms of contention: MTT's positivist constructivism},
	Volume = {31},
	Year = {2008},
	Bdsk-File-1 = {}}

