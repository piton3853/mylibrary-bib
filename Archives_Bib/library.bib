@article{Lichbach2008a,
abstract = {Doug McAdam, Sidney Tarrow, and Charles Tilly's (MTT) Dynamics of Contention is a major achievement in conflict studies and indeed the social sciences. Their goal is to ``treat social interaction, social ties, communication, and conversation not merely as expressions of structure, rationality, consciousness, or culture but as active sites of creation and change. We have come to think of interpersonal networks, interpersonal communication, and various forms of continuous negotiation---including the negotiation of identities---as figuring centrally in the dynamics of contention'' (McAdam et al. 2001 p. 22). Contention is thus innovative: it turns conventional social action into contentious politics. Contention is dynamic: multiple streams of conflict and cooperation come together. Most importantly, contention is relational: a society of individuals, and neither just individuals nor just society, characterize contention.$\backslash$nMTT are less interested in determining which variables matter in a regression equation than in exploring how these constructive social processes operate. While they suggest that variables are most often a poor substitute and shallow stand-in for the causal mechanisms that account for the dynamics, relations, and innovations that give contention its most interesting and important features, their measurement models of mechanisms of contention could potentially turn constructivist insights into hypotheses, causal accounts, and research designs. MTT thus offer us positivist constructivism---a third way between positivist correlationalism and humanist constructivism.$\backslash$nAssuming that this is indeed their agenda, I aim to remind MTT and their readers of three things. First, measuring mechanisms is only a part of thinking and working with the entities, properties, events, structures, and situations associated with the causal grittiness of mechanisms. Second, successful causal accounts combine mechanism-based explanations with variable-based explanations, linking processes to hypotheses. Finally, quantitative and qualitative research designs probe the counterfactuals and the manipulations of the world involved in causal-mechanism accounts of contention.},
author = {Lichbach, Mark Irving},
doi = {10.1007/s11133-008-9104-2},
file = {:Users/Nick/Pictures/Library/Mendeley/Lichbach{\_}Qual Sociol{\_}Modeling Mechanisms of Contention MTTs Positivist Constructivism 2008a.pdf:pdf},
isbn = {0162-0436},
issn = {01620436},
journal = {Qualitative Sociology},
number = {4},
pages = {345--354},
title = {{Modeling mechanisms of contention: MTT's positivist constructivism}},
volume = {31},
year = {2008}
}
@article{Franklin1991a,
abstract = {Campaigns play a central role in a democracy. I examine the effect of campaigns on the perception of the ideological positions of incumbent senators. The results demonstrate that incumbents affect voter perception both through their actions in office and on the campaign trail. Using the 1988 Senate Election Study, I find that the perceived location of incumbents depends on their roll call voting records, the perceived position of their party and the voter's own position. More crucial is the finding that candidates can affect the clarity of these perceptions through their campaign strategies. Incumbents who stress issues increase the clarity of voter perceptions, while challengers' attacks on incumbents reduce clarity. While elections alone increase clarity, these effects are small in comparison to the effect due to candidate campaign strategies. The results remind us that to understand the politics of elections we must incorporate candidate strategy in our models},
